#!/usr/bin/env node

/**
 * Module dependencies.
 */

const app = require("../app");
const debug = require("debug")("mission-backend:server");
const http = require("http");

/**
 * Create HTTP server.
 */

const server = http.createServer(app);

/**
 * Listen on provided port, on all network interfaces.
 */
const activePort = 4000
server.listen(activePort, () => {
  console.log(`My server start on port ${activePort}`)
});


